@import '../values' 

.Column
	height 100%
	border-right 1px solid black
	display inline-block
	position relative
	
.ColumnInner
	height 100%
	display table
	
.RowsContainer
	display table-row
	height 100%

.RowsContainerInner
	display table-cell
	vertical-align top
	position relative
	
.RowsContainerWrapper
	overflow auto
	width 100%
	height 100%
	min-width columnMinWidth
	
.selected
	background red