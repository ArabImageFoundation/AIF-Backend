body,html
	padding 0
	margin 0
body
	font 14px "Lucida Grande", Helvetica, Arial, sans-serif
   -webkit-touch-callout none
   -webkit-user-select none
   -khtml-user-select none
   -moz-user-select none
   -ms-user-select none
   user-select none
a
	color: #0084FF
	
html,body
	width 100%
	height 100%
	background #1B1B1B

hr
	width 90%
	margin .2em auto
	border 0
	border-bottom 1px solid #636262

#Title
	position absolute
	top 0
	left 0
	height 3em
	width 100%
	padding .5em
	padding-left 1em
	box-sizing border-box
	color white
	background #1A1818
	margin 0 auto
	h1
		margin 0 auto
		padding 0
		font-size 1.2em
		
#Wrapper
	position absolute
	top 3em
	left .5em
	//right 22.5em
	right .5em
	bottom .5em
	
.column-header
	h1
		font-size 1.2em
		padding 0
		margin 0
		background black
		color white
		